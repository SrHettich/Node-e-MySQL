MYSQL 
OBS: CODES NO MYSQL TERMINAM COM ';'

ENTRAR MYSQL - 'mysql - h localhost -u root -p'

mostrar database - 'SHOW DATABASES'

criar database - 'CREATE DATABASE NOME_DA_NOVADATABASE'

entrar no database - 'USE NOME_DATABASE'

criar tabela - 'CREATE TABLE NOME DA TABELA'

VER TABELAS - 'SHOW TABLES'

VER ESTRUTURA DAS TABELAS - 'DESCRIBE NOME_TABELA'

INSERIR NA TABELA - 'INSERT INTO NOME_TABELA(EXEMPLO, EXEMPLO)'

MOSTRAR INTES NA TABELA - 'select exemplo from NOME_TABELA'(USANDO * NO LUGAR DO EXEMPLO SELECIONA TUDO)

SELECIONAR ESPECIFICAMENTE -'select * from NOME_TABELA where NOME_COLUNA = / < / > SUA_BUSCA'

DELETAR DADO - 'delete from NOME_TABELA where NOME_COLUNA = / < / > = O QUE QUER DELETAR'

ATUALIZAR DADOS - update NOME_TABELA set NOME_COLUNA = "update" where NOME_COLUNA = EXEMPLO 

PARA HANDLEBARS FUNCIONAR - CRIAR PASTA 'views' dentro dela pasta 'layouts' dentro dela ARQUIVO 'main.handlebars'